    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "affected": "./node_modules/.bin/nx affected",
    "protractor:testSuites": "tsc --project ./apps/e2e/tsconfig.e2e.json & protractor ./apps/e2e/protractor.conf.js --suite issuesManagement,workManagement,performanceAnalyst,programNavigator",
    "protractor:WM": "tsc --project ./apps/e2e/tsconfig.e2e.json & protractor ./apps/e2e/protractor.conf.js --suite workManagement",
    "protractor:performanceAnalyst": "tsc --project ./apps/e2e/tsconfig.e2e.json & protractor ./apps/e2e/protractor.conf.js --suite performanceAnalyst",
    "protractor:Alert": "tsc --project ./apps/e2e/tsconfig.e2e.json & protractor ./apps/e2e/protractor.conf.js --suite Alert",
    "protractor:IM": "tsc --project ./apps/e2e/tsconfig.e2e.json & protractor ./apps/e2e/protractor.conf.js --suite issuesManagement",
    "protractor:programNavigator": "tsc --project ./apps/e2e/tsconfig.e2e.json & protractor ./apps/e2e/protractor.conf.js --suite programNavigator",
    "protractor:LV:PN": "tsc --project ./apps/e2e/tsconfig.e2e.json & protractor ./apps/e2e/protractor.conf.js --suite listviewPN",
    "protractor:map": "tsc --project ./apps/e2e/tsconfig.e2e.json & protractor ./apps/e2e/protractor.conf.js --suite maps",
    "protractor:AE": "tsc --project ./apps/e2e/tsconfig.e2e.json & protractor ./apps/e2e/protractor.conf.js --suite assetExplorer",
    "protractor:LV:RA": "tsc --project ./apps/e2e/tsconfig.e2e.json & protractor ./apps/e2e/protractor.conf.js --suite listviewRA",
    "tsc": "tsc",
    "wdupdate": "webdriver-manager update",
    "wdstart": "webdriver-manager start --detach",
    "webdriver:update": "./node_modules/protractor/bin/webdriver-manager update",
    "webdriver:start": "./node_modules/protractor/bin/webdriver-manager start --versions.chrome=2.26 --detach",
    "docker-build": "./apps/e2e/docker-build.sh"
  },
