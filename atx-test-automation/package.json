  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy-dev.conf.json",
    "start:local": "ng serve --proxy-config proxy-local.conf.json",
    "build": "ng build",
    "build-workfloweditor": "ng build workflow-editor",
    "build-workfloweditor-dev": "ng build workflow-editor --base-href /WorkflowEditor/ ",
    "build-workfloweditor-test": "ng build workflow-editor --base-href /WorkflowEditor/ --prod --configuration=test",
    "build-workfloweditor-stage": "ng build workflow-editor --base-href /WorkflowEditor/ --prod --configuration=stage",
    "build-workfloweditor-prod": "ng build workflow-editor --base-href /WorkflowEditor/ --prod",
    "build-asset360": "ng build asset360",
    "build-asset360-dev": "ng build asset360 --base-href /Asset360Dashboard/ ",
    "build-asset360-test": "ng build asset360 --base-href /Asset360Dashboard/ --prod --configuration=test",
    "build-asset360-stage": "ng build asset360 --base-href /Asset360Dashboard/ --prod --configuration=stage",
    "build-asset360-prod": "ng build asset360 --base-href /Asset360Dashboard/ --prod  --configuration=production",
    "publish-workfloweditor-dev": "@powershell -NoProfile -ExecutionPolicy Unrestricted -File ./publish.ps1 -localPath workflow-editor -site SII_WEB_DEV",
    "publish-workfloweditor-test": "@powershell -NoProfile -ExecutionPolicy Unrestricted -File ./publish.ps1 -localPath workflow-editor -site SII_WEB_TEST",
