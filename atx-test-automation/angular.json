{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "workflow-editor": {
      "root": "apps/workflow-editor/",
      "sourceRoot": "apps/workflow-editor/src",
      "projectType": "application",
      "prefix": "atx",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/workflow-editor",
            "baseHref": "/WorkflowEditor/",
            "deployUrl": "/WorkflowEditor/",
            "index": "apps/workflow-editor/src/index.html",
            "main": "apps/workflow-editor/src/main.ts",
            "polyfills": "apps/workflow-editor/src/polyfills.ts",
            "tsConfig": "apps/workflow-editor/tsconfig.app.json",
            "assets": [
              "apps/workflow-editor/src/favicon.ico",
              "apps/workflow-editor/src/web.config",
              "apps/workflow-editor/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/assets/shared-assets/",
                "output": "./assets"
              }
            ],
            "styles": ["libs/assets/src/material-styles/global.scss", "apps/workflow-editor/src/styles/main.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/assets/src/material-styles"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "build/environments/environment.ts",
                  "with": "build/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "test": {
              "fileReplacements": [
                {
                  "replace": "build/environments/environment.ts",
                  "with": "build/environments/environment.test.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "stage": {
              "fileReplacements": [
                {
                  "replace": "build/environments/environment.ts",
                  "with": "build/environments/environment.stage.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "workflow-editor:build",
            "servePath": "WorkflowEditor/",
            "baseHref": "/WorkflowEditor/"
          },
          "configurations": {
            "production": {
              "browserTarget": "workflow-editor:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "workflow-editor:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/workflow-editor/tsconfig.app.json", "apps/workflow-editor/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/workflow-editor/jest.config.js",
            "tsConfig": "apps/workflow-editor/tsconfig.spec.json",
            "setupFile": "apps/workflow-editor/src/test-setup.ts"
          }
        }
      }
    },
    "workflow-editor-e2e": {
      "root": "apps/workflow-editor-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
